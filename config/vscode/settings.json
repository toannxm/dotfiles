	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": [
					"comment.block.python",
					"comment.line.number-sign.python"
				],
				"settings": {
					"foreground": "#a29c9c",
					"fontStyle": "italic"
				}
			},
			{
				"scope": [
					"constant.language.python",
					"storage.type.class.python",
					"keyword.control.flow.python",
					"storage.type.function.python",
					"keyword.control.import.python",
					"keyword.operator.logical.python",
					"constant.character.escape.python",
					"keyword.operator.comparison.python",
					"keyword.operator.assignment.python",
					"storage.modifier.declaration.python",
					"constant.character.format.placeholder.other.python"
				],
				"settings": {
					"foreground": "#c87757",
					"fontStyle": ""
				}
			},
			{
				"scope": [
					"support.type.python",
					"support.function.builtin.python"
				],
				"settings": {
					"foreground": "#8888C6",
					"fontStyle": ""
				}
			},
			{
				"scope": [
					"support.variable.magic.python",
					"support.function.magic.python"
				],
				"settings": {
					"foreground": "#92116d",
					"fontStyle": "italic"
				}
			},
			{
				"scope": [
					"variable.language.special.self.python",
					"variable.language.special.cls.python",
					"variable.parameter.function.language.special.self.python",
					"variable.parameter.function.language.special.cls.python"
				],
				"settings": {
					"foreground": "#a6479b",
					"fontStyle": ""
				}
			},
			{
				"scope": [
					"meta.function-call.python",
					"entity.name.function.python",
					"meta.item-access.arguments.python",
					"meta.member.access.python meta.function-call.python meta.function-call.generic.python",
					"meta.function-call.python meta.function-call.arguments.python meta.function-call.python meta.function-call.generic.python"
				],
				"settings": {
					"foreground": "#4B9CD3"
				}
			},
			{
				"scope": [
					"meta.class.python",
					"meta.function.python",
					"keyword.operator.arithmetic.python"
				],
				"settings": {
					"foreground": "#A9B7C6",
					"fontStyle": ""
				}
			},
			{
				"scope": [
					"meta.function.decorator.python",
					"entity.name.function.decorator.python",
					"meta.function.decorator.python support.type.python",
					"meta.function.decorator.python entity.name.function.decorator.python"
				],
				"settings": {
					"foreground": "#BBB529",
					"fontStyle": "italic"
				}
			},
			{
				"scope": "entity.name.function.decorator.property.python",
				"settings": {
					"foreground": "#aeaea6",
					"fontStyle": "italic"
				}
			},
			{
				"scope": "entity.name.type.class.python",
				"settings": {
					"foreground": "#3386de",
					"fontStyle": "bold"
				}
			},
			{
				"scope": [
					"meta.attribute.python",
					"variable.parameter.function.language.python"
				],
				"settings": {
					"foreground": "#CCCCCC",
					"fontStyle": "italic"
				}
			},
			{
				"scope": "meta.function.parameters.python",
				"settings": {
					"foreground": "#d97085",
					"fontStyle": "italic"
				}
			},
			{
				"scope": "meta.function.parameters.python variable.parameter.optional.python",
				"settings": {
					"foreground": "#943414",
					"fontStyle": "italic underline"
				}
			},
			{
				"scope": [
					"string.quoted.single.python",
					"string.quoted.double.python",
					"variable.parameter.variadic.python",
					"string.quoted.docstring.multi.python",
					"meta.function.parameters.python variable.parameter.variadic.python",
					"meta.function-call.arguments.python variable.parameter.variadic.python",
					"meta.function-call.arguments.python meta.function-call.python support.variable.magic.python",
					"variable.parameter.function.python support.variable.magic.python"
				],
				"settings": {
					"foreground": "#629755",
					"fontStyle": "italic"
				}
			},
			{
				"scope": [
					"meta.function-call.arguments.python variable.parameter.function-call.python"
				],
				"settings": {
					"foreground": "#c04141",
					"fontStyle": "italic"
				}
			},
			{
				"scope": [
					"keyword.operator.unpacking.parameter.python",
					"keyword.operator.unpacking.arguments.python",
					"meta.function-call.arguments.python keyword.operator.unpacking.arguments.python",
					"meta.function.parameters.python keyword.operator.unpacking.parameter.python"
				],
				"settings": {
					"foreground": "#219e30"
				}
			},
			{
				"scope": "meta.function-call.arguments.python",
				"settings": {
					"foreground": "#ec7070"
				}
			},
			{
				"scope": "constant.other.caps.python",
				"settings": {
					"foreground": "#ef9e85"
				}
			},
			{
				"scope": [
					"constant.numeric.integer.python",
					"constant.numeric.float.python",
					"constant.numeric.hex.python",
					"constant.numeric.bin.python",
					"constant.numeric.oct.python"
				],
				"settings": {
					"foreground": "#6897BB",
					"fontStyle": ""
				}
			},
			{
				"scope": "source.env variable.other.env",
				"settings": {
					"foreground": "#dfa15a",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "source.env comment.line.number-sign.env",
				"settings": {
					"foreground": "#949290"
				}
			},
			{
				"scope": [
					"source.env",
					"source.env string.quoted.double.env",
					"source.env string.quoted.single.env",
					"source.env constant.numeric.env"
				],
				"settings": {
					"foreground": "#48b345"
				}
			}
		]
	},
